{
  "1": {
    "endpoint": "Get all the colors",
    "request": {
      "method": "GET",
      "URL": "/colors",
      "headers": false,
      "body": false
    },
    "response": {
      "headers": {"content-type": "application/json"},
      "statusCode": 200,
      "body": [{"colorId": true, "color": true}]
    }
  },
  "2": {
    "endpoint": "Add a color",
    "request": {
      "method": "POST",
      "URL": "/colors",
      "headers": {"content-type": "application/json"},
      "body": {"color": true}
    },
    "response": {
      "headers": {"content-type": "application/json"},
      "statusCode": 201,
      "body": {"colorId": true, "color": true}
    }
  }
}
