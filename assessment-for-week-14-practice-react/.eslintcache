ame, sweetness, color, seeds]);\n\n\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    const newFruit = {\n      name,\n      sweetness,\n      color,\n      seeds\n    }\n    console.log(newFruit);\n    // history.push('/')\n    setRedirectNeeded(true);\n  }\n\n\n  return (\n\n      <form\n        className=\"fruit-form\"\n        onSubmit={handleSubmit}\n      >\n        <h2>Enter a Fruit</h2>\n        {redirectNeeded && <Redirect to='/' />}\n        <ul className=\"errors\">\n          {validationErrors.length > 0 && validationErrors.map((error, i) => (\n            <li key={i}>{error}</li>\n          ))}\n        </ul>\n        <label>\n          Name\n          <input\n            type=\"text\"\n            name=\"name\"\n            value={name}\n            onChange={(e) => setName(e.target.value)}\n          />\n        </label>\n        <label>\n          Select a Color\n          <select\n          value={color}\n          onChange={(e) => setColor(e.target.value)}\n          >\n            {COLORS.map(color => (\n              <option\n                key={color}\n                value={color}\n              >\n                {color}\n              </option>\n            ))}\n          </select>\n        </label>\n        <label>\n          Sweetness\n          <input\n            type=\"number\"\n            name=\"sweetness\"\n            value={sweetness}\n            onChange={(e) => setSweetness(e.target.value)}\n          />\n        </label>\n        <label>\n          <input\n            type=\"radio\"\n            value=\"no\"\n            name=\"seeds\"\n            checked={seeds === 'no'}\n            onChange={(e) => setSeeds(e.target.value)}\n          />\n          No Seeds\n        </label>\n        <label>\n          <input\n            type=\"radio\"\n            value=\"yes\"\n            name=\"seeds\"\n            checked={seeds === 'yes'}\n            onChange={(e) => setSeeds(e.target.value)}\n          />\n          Seeds\n        </label>\n        <button\n          type=\"submit\"\n          disabled={validationErrors.length > 0}\n        >\n          Submit Fruit\n        </button>\n      </form>\n\n  );\n}\n\nexport default FruitForm;\n","/home/casey/my-aa-folder/git-repo-folder/assessment-for-week-14-practice-react/src/components/FavoriteFruit.js",["33"],{"ruleId":"34","severity":1,"message":"35","line":1,"column":35,"nodeType":"36","messageId":"37","endLine":1,"endColumn":45},{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","severity":1,"message":"43","line":31,"column":6,"nodeType":"44","endLine":31,"endColumn":37,"suggestions":"45"},{"ruleId":"34","severity":1,"message":"46","line":2,"column":10,"nodeType":"36","messageId":"37","endLine":2,"endColumn":25},"no-unused-vars","'useContext' is defined but never used.","Identifier","unusedVar","no-native-reassign",["47"],"no-negated-in-lhs",["48"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'fruits'. Either include it or remove the dependency array.","ArrayExpression",["49"],"'FavFruitContext' is defined but never used.","no-global-assign","no-unsafe-negation",{"desc":"50","fix":"51"},"Update the dependencies array to be: [name, sweetness, color, seeds, fruits]",{"range":"52","text":"53"},[1045,1076],"[name, sweetness, color, seeds, fruits]"]